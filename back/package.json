    "windowsMigration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "windowsMigration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "windowsMigration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert"
